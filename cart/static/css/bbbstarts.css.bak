	100% { margin-top:100px; }
	
}

@-webkit-keyframes kolor {
	0% {background-color: #ebbfad; }
	33% {background-color: #572FBC; }
	
	66% {background-color: #2FBC57; }
	100% {background-color: #ebbfad; }
	
	
}

